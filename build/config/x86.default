# => -DPLATFORM_X86
HILDA_PLATFORM := x86
# yes => -g3, no => -O3
HILDA_DEBUG := yes

# Will Add the $PATH
GCC_PATH +=

INCDIR += -I /usr/include

LIBS += -L /usr/lib -lpthread -lrt -ldl -lexpat 
 
# DEFINES += -DAAA -DBBB
DEFINES += -D_REENTRANT 

CFLAGS += -fno-strict-aliasing -Wall
LDFLAGS +=

CROSS_COMPILE := 
CC := ccache $(CROSS_COMPILE)gcc
AR := $(CROSS_COMPILE)ar
LD := $(CROSS_COMPILE)ld
RM = rm
OS = linux

